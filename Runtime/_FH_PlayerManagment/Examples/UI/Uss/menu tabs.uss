.PlayerBackground {
    background-image: none;
    background-color: rgb(255, 255, 255);
}

.Popup {
    -unity-background-scale-mode: stretch-to-fill;
    position: absolute;
    flex-grow: 0;
    width: 95%;
    height: 95%;
    background-color: rgb(255, 255, 255);
    background-image: none;
    border-top-left-radius: 73px;
    border-top-right-radius: 73px;
    border-bottom-right-radius: 73px;
    border-bottom-left-radius: 73px;
    border-top-width: 9px;
    border-right-width: 9px;
    border-bottom-width: 9px;
    border-left-width: 9px;
    border-left-color: rgb(177, 184, 204);
    border-right-color: rgb(177, 184, 204);
    border-top-color: rgb(177, 184, 204);
    border-bottom-color: rgb(177, 184, 204);
    overflow: hidden;
    padding-top: 7px;
    padding-right: 7px;
    padding-bottom: 7px;
    padding-left: 7px;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    opacity: 1;
    top: 2.5%;
    left: 2.5%;
    right: 2.5%;
    bottom: 2.5%;
}

.lobby-list-ui {
    background-image: url("project://database/Assets/_FH_PlayerManagment/Examples/UI/box.png?fileID=2800000&guid=8632e61e698c0b44d8da7446ba122a53&type=3#box");
    flex-grow: 1;
