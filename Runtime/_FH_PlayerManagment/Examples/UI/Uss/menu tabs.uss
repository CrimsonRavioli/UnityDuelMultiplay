:root {
}

.unity-tab__header {
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgb(255, 255, 255);
    background-image: none;
    border-top-left-radius: 29px;
    border-top-right-radius: 29px;
    border-bottom-right-radius: 29px;
    border-bottom-left-radius: 29px;
}

.unity-tab__header:checked {
}

.tab-content {
}

.unity-tab__header-underline {
}

.unity-button {
    background-image: none;
    background-color: rgb(226, 226, 226);
    -unity-background-image-tint-color: rgb(76, 76, 76);
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
}

.unity-button:hover {
    -unity-background-image-tint-color: rgb(0, 0, 0);
    transition-duration: 0.2s;
    background-size: 110% 110%;
    scale: 1.02 1.02;
    color: rgb(255, 255, 255);
    background-color: rgb(0, 0, 0);
}

.unity-button:active {
    -unity-background-image-tint-color: rgb(70, 90, 170);
}

.unity-button:disabled {
    -unity-background-image-tint-color: rgb(50, 50, 50);
    color: rgb(100, 100, 100);
}

.unity-popup-field .unity-button {
    background-image: url('project://database/Assets/Layer%20Lab/GUI%20Pro-SimpleCasual/ResourcesData/Sprites/Components/Frame/Frame_Demo_Common/BasicFrame_Square02_Red.png?fileID=2800000&guid=a07c5b3907ec843a8bca61176e91e92d&type=3#BasicFrame_Square02_Red');
    background-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: rgb(120, 153, 207);
}

.unity-popup-field .unity-button:hover {
    -unity-background-image-tint-color: rgb(95, 115, 195);
}

.unity-popup-field .unity-button:active {
    -unity-background-image-tint-color: rgb(70, 90, 170);
}

.unity-base-field {
    -unity-background-image-tint-color: rgb(255, 255, 255);
}

.unity-base-text-field {
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(0, 0, 0, 0);
}

